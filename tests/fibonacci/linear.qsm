func fibonacci(n: i32) -> i32 {
    let a = 0
    let b = 1
    let next = 1
    
    while n > 0 {
        next = a + b
        a = b
        b = next

        n = n - 1
    }

    return a
}

func main() {
    let n = 20
    let i = 0
    
    while i <= n {
        let result = fibonacci(i)
        print(result)
        i = i + 1
    }
}
